{
  "name": "Oshocks Junior Bike Shop - Kenya's Premier Cycling Marketplace",
  "short_name": "Oshocks",
  "description": "Discover thousands of bicycles, cycling accessories, and spare parts. Kenya's leading online marketplace for cycling enthusiasts with M-Pesa payments, fast nationwide delivery, and quality guaranteed products.",
  "version": "2.0.0",
  "manifest_version": 3,
  
  "start_url": "/?source=pwa",
  "scope": "/",
  "display": "standalone",
  "display_override": ["window-controls-overlay", "standalone", "minimal-ui"],
  "orientation": "portrait-primary",
  
  "theme_color": "#667eea",
  "background_color": "#ffffff",
  
  "categories": [
    "shopping",
    "sports",
    "lifestyle",
    "business"
  ],
  
  "lang": "en-KE",
  "dir": "ltr",
  
  "icons": [
    {
      "src": "oshocks-pwa.png",
      "type": "image/png",
      "sizes": "192x192",
      "purpose": "any"
    },
    {
      "src": "apple-touch-icon.png",
      "type": "image/png",
      "sizes": "180x180",
      "purpose": "any"
    }
  ],
  
  "screenshots": [
    {
      "src": "screenshots/desktop-home.png",
      "type": "image/png",
      "sizes": "1920x1080",
      "form_factor": "wide",
      "label": "Oshocks Homepage - Browse thousands of cycling products"
    },
    {
      "src": "screenshots/desktop-shop.png",
      "type": "image/png",
      "sizes": "1920x1080",
      "form_factor": "wide",
      "label": "Shop Page - Filter and find your perfect bike"
    },
    {
      "src": "screenshots/desktop-product.png",
      "type": "image/png",
      "sizes": "1920x1080",
      "form_factor": "wide",
      "label": "Product Details - Complete specifications and reviews"
    },
    {
      "src": "screenshots/mobile-home.png",
      "type": "image/png",
      "sizes": "750x1334",
      "form_factor": "narrow",
      "label": "Mobile Home - Shop on the go"
    },
    {
      "src": "screenshots/mobile-cart.png",
      "type": "image/png",
      "sizes": "750x1334",
      "form_factor": "narrow",
      "label": "Shopping Cart - Easy checkout with M-Pesa"
    },
    {
      "src": "screenshots/mobile-checkout.png",
      "type": "image/png",
      "sizes": "750x1334",
      "form_factor": "narrow",
      "label": "Secure Checkout - Multiple payment options"
    }
  ],
  
  "shortcuts": [
    {
      "name": "Browse All Bikes",
      "short_name": "Bikes",
      "description": "Explore our complete collection of bicycles",
      "url": "/shop?category=bicycles",
      "icons": [
        {
          "src": "icons/bike-96.png",
          "sizes": "96x96",
          "type": "image/png"
        },
        {
          "src": "icons/bike-192.png",
          "sizes": "192x192",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Mountain Bikes",
      "short_name": "MTB",
      "description": "Browse mountain bikes for all terrains",
      "url": "/shop?category=mountain-bikes",
      "icons": [
        {
          "src": "icons/mtb-96.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Shopping Cart",
      "short_name": "Cart",
      "description": "View your shopping cart and checkout",
      "url": "/cart",
      "icons": [
        {
          "src": "icons/cart-96.png",
          "sizes": "96x96",
          "type": "image/png"
        },
        {
          "src": "icons/cart-192.png",
          "sizes": "192x192",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "My Orders",
      "short_name": "Orders",
      "description": "Track and manage your orders",
      "url": "/orders",
      "icons": [
        {
          "src": "icons/orders-96.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Wishlist",
      "short_name": "Wishlist",
      "description": "View your saved favorite items",
      "url": "/wishlist",
      "icons": [
        {
          "src": "icons/wishlist-96.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Search Products",
      "short_name": "Search",
      "description": "Search for bikes, accessories, and parts",
      "url": "/search",
      "icons": [
        {
          "src": "icons/search-96.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Today's Deals",
      "short_name": "Deals",
      "description": "Browse today's special offers and discounts",
      "url": "/deals",
      "icons": [
        {
          "src": "icons/deals-96.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Customer Support",
      "short_name": "Support",
      "description": "Get help and contact customer service",
      "url": "/contact",
      "icons": [
        {
          "src": "icons/support-96.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    }
  ],
  
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "image",
          "accept": ["image/jpeg", "image/png", "image/webp", "image/gif"]
        }
      ]
    }
  },
  
  "protocol_handlers": [
    {
      "protocol": "web+oshocks",
      "url": "/product?id=%s"
    },
    {
      "protocol": "web+oshockscart",
      "url": "/cart?action=%s"
    }
  ],
  
  "file_handlers": [
    {
      "action": "/import",
      "accept": {
        "text/csv": [".csv"],
        "application/json": [".json"]
      }
    }
  ],
  
  "launch_handler": {
    "client_mode": ["navigate-existing", "auto"]
  },
  
  "edge_side_panel": {
    "preferred_width": 400
  },
  
  "related_applications": [
    {
      "platform": "play",
      "url": "https://play.google.com/store/apps/details?id=com.oshocks.app",
      "id": "com.oshocks.app"
    },
    {
      "platform": "itunes",
      "url": "https://apps.apple.com/app/oshocks/id123456789"
    }
  ],
  "prefer_related_applications": false,
  
  "iarc_rating_id": "",
  
  "serviceworker": {
    "src": "/service-worker.js",
    "scope": "/",
    "use_cache": true
  },
  
  "features": [
    "Cross-platform PWA support",
    "Offline browsing capability",
    "Push notifications for orders and deals",
    "Background sync for cart and wishlist",
    "M-Pesa payment integration",
    "Real-time order tracking",
    "Product search and filters",
    "User reviews and ratings",
    "Live chat support",
    "Multi-vendor marketplace"
  ],
  
  "gcm_sender_id": "103953800507",
  
  "permissions": [
    "notifications",
    "geolocation"
  ],
  
  "note_taking": {
    "new_note_url": "/notes/new"
  },
  
  "handle_links": "preferred",
  
  "capture_links": "existing-client-navigate",
  
  "url_handlers": [
    {
      "origin": "https://oshocks.co.ke"
    },
    {
      "origin": "https://www.oshocks.co.ke"
    }
  ],
  
  "scope_extensions": [],
  
  "display_capture": "fullscreen",
  
  "id": "oshocks-bike-shop-kenya",
  
  "app": {
    "background": {
      "scripts": ["background.js"]
    },
    "content_scripts": [
      {
        "matches": ["https://oshocks.co.ke/*"],
        "js": ["content-script.js"]
      }
    ]
  }
}